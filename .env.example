# TradLab Environment Configuration
# Copy this file to .env and fill in your values

# ============================================================================
# PostgreSQL Database
# ============================================================================
# Password for PostgreSQL (used by docker-compose)
POSTGRES_PASSWORD=YOUR_PASSWORD_HERE

# Full database connection string
DATABASE_URL=postgresql://tradlab:YOUR_PASSWORD_HERE@localhost:5432/tradlab_db

# ============================================================================
# Backtest Settings
# ============================================================================
TRADLAB_INITIAL_CAPITAL=10000
TRADLAB_COMMISSION_RATE=0.0004
TRADLAB_SLIPPAGE_BPS=5
